
public class SubjectAdditions {
	public static void main (String[] args) {

		int intPhys, intChem, intMath;
		int intTotal;
		intPhys = 75;
		intChem = 92;
		intMath = 100;
		intTotal = intPhys + intChem + intMath;
		System.out.print("Phys is: " + intPhys); System.out.print(", Chem is: " + intChem); System.out.println(", Math is: " + intMath); 

		System.out.println("Total is: " + (intPhys + intChem + intMath));
	}
}
