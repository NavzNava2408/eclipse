
public class IfAndElse {
	public static void main(String[] args) {
		int temp = 40;
		if (temp>= 40) {
			System.out.println("It");
			System.out.print(" is");
			System.out.print(" warm!");
		}
		else {
			System.out.println("It");
			System.out.print(" is");
			System.out.print(" cold!");
		}
	}

}
